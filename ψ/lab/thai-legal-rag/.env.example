# Copy to .env and fill in values

# Gemini API keys (comma-separated for rotation)
GEMINI_API_KEYS=your-key-1,your-key-2

# Google Drive service account (either JSON string or path to file)
GOOGLE_SERVICE_ACCOUNT_JSON=/path/to/service_account.json

# Drive folder IDs per category
DRIVE_FOLDER_CGD=your-folder-id-for-cgd
DRIVE_FOLDER_ADMIN_COURT=your-folder-id-for-admin-court
